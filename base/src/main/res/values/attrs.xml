<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="title_size" format="dimension" />
    <attr name="title_color" format="color" />
    <declare-styleable name="TopNavigationWidgets">
        <!--    标题加粗，默认中粗 0.8，不加粗设置为0    -->
        <attr name="title_stroke_width" format="float" />
        <!--    标题左右margin值，默认60，单行显示，...    -->
        <attr name="textMargin" format="dimension" />
        <!--    返回按钮左边距    -->
        <attr name="backMargin" format="dimension" />
        <attr name="hide_back" format="boolean" />
        <attr name="back_drawable" format="reference" />
        <attr name="is_show_bottom_line" format="boolean" />
        <attr name="line_color" format="color" />
        <attr name="line_height" format="dimension" />
        <attr name="title" />
        <attr name="title_color" />
        <attr name="title_size" />
    </declare-styleable>
    <declare-styleable name="MediumBoldTextView">
        <attr name="tv_stroke_width" format="float" />
    </declare-styleable>
</resources>